{
  "name": "threads-automation-puppeteer",
  "version": "1.0.0",
  "description": "Multi-account Threads automation with Puppeteer",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "start": "npx tsx src/main.ts",
    "dev": "npx tsx --watch src/main.ts",
    "debug": "npx tsx --inspect src/main.ts",
    "debug:break": "npx tsx --inspect-brk src/main.ts",
    "debug:test": "node debug.js"
  },
  "keywords": [
    "puppeteer",
    "automation",
    "threads",
    "multi-account"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ghost-cursor": "^1.1.9",
    "lucide-react": "^0.543.0",
    "node-fetch": "^3.3.2",
    "puppeteer-core": "^24.17.1"
  }
}
